{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "Node16",
    "target": "es2020",
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "skipLibCheck": true,
    "noUnusedParameters": false,
    "noUnusedLocals": false
  },
  "include": ["src"]
}
